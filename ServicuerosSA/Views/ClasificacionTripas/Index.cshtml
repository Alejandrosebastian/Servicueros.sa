@model IEnumerable<ServicuerosSA.Models.ClasificacionTripa>

@{
    ViewData["Title"] = "Index";
}

<h2>Lista en tripa</h2>

<p>
    <a asp-action="Create">Ingreso una nueva calsificacion</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Detalle)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Detalle)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ClasificacionTripaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ClasificacionTripaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ClasificacionTripaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
