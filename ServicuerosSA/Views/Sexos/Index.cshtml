@model IEnumerable<ServicuerosSA.Models.Sexo>

@{
    ViewData["Title"] = "SEXO  ";
}

<h2>Sexo</h2>

<p>
    <a asp-action="Create">Ingrese un nuevo sexo</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Detalle)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Detalle)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SexoId">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.SexoId">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.SexoId">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
