@model ServicuerosSA.Models.Clasificacion

@{
    ViewData["Title"] = "Create";
}

<h2>Ingreso de nueva clasificacion</h2>

<h4>Clasificacion</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Selecciones" class="control-label"></label>
                <input asp-for="Selecciones" class="form-control" />
                <span asp-validation-for="Selecciones" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-primary" />
                <a class="btn btn-success" asp-action="Index">Regresar a la lista de clasificaciones</a>
            </div>
        </form>
    </div>
</div>

<div>
    
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
